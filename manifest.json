{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "My extension description",
  "content_security_policy":{"sandbox":"sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"},
  "permissions":["tabs","proxy","alarms","storage","cookies","debugger","downloads","scripting","contextMenus","clipboardRead","notifications","webNavigation","unlimitedStorage"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
